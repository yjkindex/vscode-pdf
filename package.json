{
  "name": "vscode-pdf",
  "displayName": "PDF Viewer",
  "description": "Portable document format (PDF) viewer for Visual Studio Code.",
  "version": "0.1.7",
  "publisher": "mathematic",
  "repository": {
    "type": "git",
    "url": "https://github.com/mathematic-inc/vscode-pdf.git"
  },
  "engines": {
    "vscode": "^1.96.0"
  },
  "icon": "icon.png",
  "license": "Apache-2.0",
  "keywords": [
    "pdf",
    "viewer"
  ],
  "categories": [
    "Visualization",
    "Other"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "customEditors": [
      {
        "viewType": "pdf.view",
        "displayName": "PDF View",
        "selector": [
          {
            "filenamePattern": "*.pdf"
          }
        ]
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "build": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "test-compile": "npm run compile",
    "test-watch": "npm run watch"
  },
  "devDependencies": {
    "@types/node": "22.x",
    "@types/vscode": "1.96.0",
    "ts-loader": "9.5.2",
    "typescript": "5.7.3",
    "vsce": "^2.15.0",
    "webpack": "5.97.1",
    "webpack-cli": "6.0.1"
  }
}
